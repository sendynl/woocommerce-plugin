{
	"name": "sendynl/woocommerce-plugin",
	"license": "MIT",
	"type": "wordpress-plugin",
	"require": {
		"sendynl/php-sdk": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Sendy\\WooCommerce\\": "lib/"
		}
	},
	"config": {
		"platform": {
			"php": "7.4.0"
		}
	},
	"scripts": {
		"lint": "vendor/bin/php-cs-fixer fix --dry-run --diff",
		"fix": "vendor/bin/php-cs-fixer fix"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.92"
	}
}
